# .bashrc

alias maubot-start="sudo systemctl start maubot.service && echo 'maubot is running'"
alias maubot-logs="sudo journalctl -u maubot.service"
alias maubot-restart="sudo systemctl restart maubot.service && echo 'maubot is running'"
alias maubot-stop="sudo systemctl stop maubot.service && echo 'maubot stopped'"
alias maubot-reinstall-maubot-fedora="pushd /home/vagrant/maubot/maubot-fedora/; mbc build -u; popd;"
